		<Documentation>
			<Author Licence="LGPL" Name="Zheng, Lei" EMail="realthunder.dev@gmail.com" />
			<UserDocu>Extension class to impelement object linking</UserDocu>
		</Documentation>
        <Methode Name="setLink">
            <Documentation>
                <UserDocu>
                    setLink(name, filename=None, realtive=True): set the link.\n
                    \nname(string): the object name
                    \nfilename(string): the document path.  None means the object is in the same document.
                    \nrealtive(bool): If ture, convert the path to realtive path against the owner document of this link object.
                    \nReturns the object linked if found. It will try to open the refered the document if it is not loaded.
                </UserDocu>
            </Documentation>
        </Methode>
        <Methode Name="getLinkedDocumentPath">
            <Documentation>
                <UserDocu>Returned the linked document file path</UserDocu>
            </Documentation>
        </Methode>
        <Methode Name="getLinkedObjectName">
            <Documentation>
                <UserDocu>Returned the linked object name</UserDocu>
            </Documentation>
        </Methode>
        <CustomAttributes />
    </PythonExport>
